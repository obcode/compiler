digraph finite_state_machine {
    rankdir=LR;
    size="3,3"
    node [shape = point, color=white, fontcolor=white]; start;
    node [shape = doublecircle, color=black, fontcolor=black]; S4;
    node [shape = circle];
    start -> S0;
    S0 -> S1 [ label = "/" ];
    S1 -> S2 [ label = "*" ];
    S2 -> S2 [ label = "^*" ];
    S2 -> S3 [ label = "*" ];
    S3 -> S3 [ label = "*" ];
    S3 -> S2 [ label = "^(*|/)" ];
    S3 -> S4 [ label = "/" ];
}
