digraph finite_state_machine {
    rankdir=LR;
    size="8,5"
    node [shape = point, color=white, fontcolor=white]; start;
    node [shape = doublecircle, color=black, fontcolor=black]; S9;
    node [shape = circle];
    start -> S0;
    S0 -> S1 [ label = "a" ];
    S1 -> S8 ;
    S8 -> S6 ;
    S6 -> S2 ;
    S6 -> S4 ;
    S2 -> S3 [ label = "b" ];
    S4 -> S5 [ label = "c" ];
    S3 -> S7 ;
    S5 -> S7 ;
    S7 -> S9 ;
    S8 -> S9 ;
    S7 -> S6 ;
}
