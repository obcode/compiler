digraph finite_state_machine {
    rankdir=LR;
    size="2,2"
    node [shape = point, color=white, fontcolor=white]; start;
    node [shape = doublecircle, color=black, fontcolor=black]; S1, S2;
    node [shape = circle];
    start -> S0;
    S0 -> S1 [ label = "0" ];
    S0 -> S2 [ label = "1...9" ];
    S2 -> S2 [ label = "0...9" ];
}
